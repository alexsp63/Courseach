<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="180.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.controllers.AuthorizationController">
    <children>
        <Group layoutX="213.0" layoutY="150.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
            <children>
                <Button layoutX="56.0" layoutY="86.0" mnemonicParsing="false" onAction="#signInButton" prefHeight="32.0"
                        prefWidth="114.0" stylesheets="@../style/myStyle.css" text="Вход"/>
                <Button layoutX="56.0" layoutY="118.0" mnemonicParsing="false" onAction="#signUpButton"
                        prefHeight="32.0" prefWidth="114.0" styleClass="button1" stylesheets="@../style/myStyle.css"
                        text="Регистрация"/>
                <TextField fx:id="loginText" layoutX="1.0" layoutY="8.0" prefHeight="32.0" prefWidth="225.0"/>
                <PasswordField fx:id="passwordText" layoutX="1.0" layoutY="48.0" prefHeight="32.0" prefWidth="225.0"/>
            </children>
        </Group>
    </children>
</AnchorPane>
